{
	"luci-app-transmission": {
		"description": "Grant UCI access for luci-app-transmission",
		"read": {
			"file": {
				"/bin/df": [ "exec" ],
				"/usr/share/transmission/*": [ "list" ],
				"/usr/bin/transmission-daemon": ["exec"]
			},
			"uci": [ "transmission" ]
		},
		"write": {
			"file": {
				"/*/transmission.log": [ "write" ]
			},
			"uci": [ "transmission" ]
		}
	}
}
