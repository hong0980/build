{
	"luci-app-qbittorrent": {
		"description": "Grant UCI access for luci-app-qbittorrent",
		"read": {
			"ubus": {
				"service": [ "list" ]
			},
			"file": {
				"/bin/df": [ "exec" ],
				"/bin/pidof": [ "exec" ],
				"/usr/bin/env": [ "exec" ],
				"/sbin/logread": [ "exec" ]
			},
			"uci": [ "qbittorrent" ]
		},
		"write": {
			"file": {
				"/*/qbittorrent.log": [ "write" ]
			},
			"uci": [ "qbittorrent" ]
		}
	}
}
