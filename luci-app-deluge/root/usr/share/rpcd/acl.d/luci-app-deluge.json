{
	"luci-app-deluge": {
		"description": "Grant access to Deluge configuration",
		"read": {
			"uci": [ "deluge" ],
			"file": {
				"/bin/df": [ "exec" ],
				"/sbin/logread": [ "exec" ],
				"/usr/bin/deluge": [ "exec" ],
				"/usr/bin/pgrep": [ "exec" ]
			}
		},
		"write": {
			"file": {
				"/*/deluge.log": [ "write" ]
			},
			"uci": [ "deluge" ]
		}
	}
}
