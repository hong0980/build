
config eqosplus 'config'

config device
	option enable '0'
	option mac ''
	option download '1'
	option upload '1'
	option timestart '00:00'
	option timeend '23:55'
	option week '0'

config device
	option enable '0'
	option mac '192.168.10.10/24'
	option download '0.1'
	option upload '0.1'
	option timestart '00:00'
	option timeend '00:00'
	option week '0'

