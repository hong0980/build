{
	"luci-app-taskplan": {
		"description": "Grant access for luci-app-taskplan",
		"read": {
			"file": {
				"/bin/sh": ["exec"],
				"/bin/bash": ["exec"],
				"/usr/bin/python3": ["exec"],
				"/usr/bin/which": [ "exec"],
				"/etc/taskplan/*": ["exec"],
				"/etc/taskplan/*": ["read"],
				"/etc/init.d/cron": [ "exec"],
				"/var/run/taskplan_counter.dat": ["read"]
			}
		},
		"write": {
			"file": {
				"/etc/taskplan/*": ["write"],
				"/var/run/taskplan_counter.dat": ["write"]
			}
		}
	}
}
