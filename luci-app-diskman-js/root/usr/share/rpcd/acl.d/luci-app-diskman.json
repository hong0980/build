{
	"luci-app-diskman": {
		"description": "Grant access for luci-app-diskman",
		"read": {
			"ubus": {
				"service": [ "list" ]
			},
			"file": {
				"/usr/bin/mkfs.btrfs": ["exec"],
				"/usr/bin/mkfs.fat": [ "exec" ],
				"/usr/sbin/mkfs.f2fs": [ "exec" ],
				"/usr/sbin/mkfs.ext2": [ "exec" ],
				"/usr/sbin/mkfs.ext3": [ "exec" ],
				"/usr/sbin/mkfs.ext4": ["exec"],
				"/usr/sbin/mkfs.exfat": [ "exec" ],
				"/bin/df": [ "exec"],
				"/bin/mount": ["exec"],
				"/bin/umount": ["exec"],
				"/sbin/parted": ["exec"],
				"/sbin/partprobe": ["exec"],
				"/usr/bin/btrfs": ["exec"],
				"/usr/bin/lsblk": ["exec"],
				"/usr/bin/which": ["exec"],
				"/usr/sbin/blkid": ["exec"],
				"/usr/sbin/smartctl": ["exec"],
				"/usr/libexec/diskman": ["exec"]
			}
		},
		"write": {
			"uci": [ "diskman" ]
		}
	}
}
