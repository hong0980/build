{
	"luci-app-diskman": {
		"description": "Grant access for luci-app-diskman",
		"read": {
			"ubus": {
				"service": [ "list" ]
			},
			"file": {
				"/bin/df": [ "exec" ],
				"/bin/echo": [ "exec" ],
				"/bin/mkdir": [ "exec" ],
				"/bin/mount": [ "exec" ],
				"/bin/umount": [ "exec" ],
				"/sbin/parted": [ "exec" ],
				"/usr/bin/btrfs": [ "exec" ],
				"/usr/bin/lsblk": [ "exec" ],
				"/sbin/partprobe": [ "exec" ],
				"/usr/sbin/blkid": [ "exec" ],
				"/usr/sbin/sfdisk": [ "exec" ],
				"/usr/sbin/smartctl": [ "exec" ],
				"/usr/sbin/mkfs.fat": [ "exec" ],
				"/usr/bin/mkfs.btrfs": [ "exec" ],
				"/usr/sbin/mkfs.ext4": [ "exec" ],
				"/usr/sbin/mkfs.f2fs": [ "exec" ],
				"/usr/sbin/mkfs.ext2": [ "exec" ],
				"/usr/sbin/mkfs.ext3": [ "exec" ],
				"/usr/libexec/diskman": [ "exec" ],
				"/usr/sbin/mkfs.exfat": [ "exec" ]
			}
		},
		"write": {
			"uci": [ "diskman" ]
		}
	}
}
