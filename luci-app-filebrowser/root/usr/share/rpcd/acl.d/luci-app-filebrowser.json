{
	"luci-app-filebrowser": {
		"description": "File browser with full read/write operations",
		"read": {
			"cgi-io": [ "backup", "download" ],
			"file": {
				"/bin/sh": [ "exec" ],
				"/bin/ls": [ "exec" ],
				"/bin/mv": [ "exec" ],
				"/bin/rm": [ "exec" ],
				"/bin/ln": [ "exec" ],
				"/bin/mkdir": [ "exec" ],
				"/bin/chmod": [ "exec" ],
				"/usr/bin/du": [ "exec" ],
				"/dev/mtdblock[0-9]*": [ "read" ],
				"/*": [ "list", "read", "stat" ]
			},
			"ubus": {
				"file": [ "exec", "read", "stat" ]
			},
			"uci": [ "filebrowser" ]
		},
		"write": {
			"cgi-io": [ "upload" ],
			"file": {
				"/*": [ "write", "remove" ]
			},
			"ubus": {
				"file": [ "exec", "remove", "write" ],
			},
			"uci": [ "filebrowser" ]
		}
	}
}
